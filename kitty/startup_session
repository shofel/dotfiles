#

new_tab student
cd ~/workspaces/Examus/examus-extension/
launch nix-shell --run "nvim +'Term fish'"

new_tab compose
launch fish -c 'nvim ~/tmp/drafts-(date +%Y-%m-%-d)'

new_tab student-o
cd ~/workspaces/Examus/examus-extension-other/
launch nix-shell --run "nvim +'Term fish'"

new_tab dotfiles
cd ~/workspaces/dotfiles/
launch nvim +'Term fish'

new_tab ci@student
launch nvim +'Term fish'
